{
  "settings": {
    "index.refresh_interval": "5s"
  },
  "mappings": {
    "properties": {
      "click_time": {
        "type": "date",
        "format": "yyyy-MM-dd HH:mm:ss.SSS||yyyy-MM-dd HH:mm:ss.SS||yyyy-MM-dd HH:mm:ss.S||yyyy-MM-dd HH:mm:ss.SSSZ||date_optional_time||epoch_millis"
      },
      "query_time": {
        "type": "date",
        "format": "yyyy-MM-dd HH:mm:ss.SSS||yyyy-MM-dd HH:mm:ss.SS||yyyy-MM-dd HH:mm:ss.S||yyyy-MM-dd HH:mm:ss.SSSZ||date_optional_time||epoch_millis"
      },
      "category": {
        "type" : "keyword",
        "ignore_above" : 256
      },
      "query" : {
        "type" : "text",
        "analyzer": "english",
        "fields" : {
          "keyword" : {
            "type" : "keyword",
            "ignore_above" : 512
          }
        }
      },
      "sku" : {
        "type" : "keyword",
        "ignore_above" : 256
      },
      "user" : {
        "type" : "keyword",
        "ignore_above" : 256
      }
    }
  }
}